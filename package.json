    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "colord": "^2.9.3",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "es-toolkit": "^1.39.3",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "keepalive-for-react": "^4.0.2",
    "motion": "^12.15.0",
    "nprogress": "^0.2.0",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.5.2",
    "react-router": "^7.6.1",
    "zustand": "^5.0.5"
