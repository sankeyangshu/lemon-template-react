{
  "name": "lemon-template-react",
  "version": "1.1.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "description": "An mobile web apps template based on the React ecosystem",
  "author": {
    "name": "sankeyangshu",
    "email": "sankeyangshu@gmail.com",
    "url": "https://github.com/sankeyangshu"
  },
  "license": "MIT",
  "homepage": "https://github.com/sankeyangshu/lemon-template-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sankeyangshu/lemon-template-react.git"
  },
  "bugs": {
    "url": "https://github.com/sankeyangshu/lemon-template-react/issues"
  },
  "keywords": [
    "react",
    "template",
    "typescript",
    "axios",
    "react-router",
    "react-vant",
    "i18n-react",
    "zustand",
    "unocss"
  ],
  "engines": {
    "node": "^18.18.0 || >=20.0.0",
    "pnpm": ">=8.15.4"
  },
  "scripts": {
    "dev": "vite",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:test": "vite build --mode test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "preview": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "release": "bumpp",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@react-vant/icons": "^0.1.0",
    "ahooks": "^3.8.4",
    "axios": "^1.8.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "keepalive-for-react": "^4.0.2",
    "nprogress": "^0.2.0",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.1",
    "react-router": "^7.2.0",
    "react-transition-group": "^4.4.5",
    "react-vant": "^3.3.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@iconify/react": "^5.2.0",
    "@sankeyangshu/eslint-config": "^0.2.2",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.13.9",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-transition-group": "^4.4.12",
    "@unocss/eslint-plugin": "^66.0.0",
    "@unocss/preset-rem-to-px": "^66.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "less": "^4.2.2",
    "lint-staged": "^15.4.3",
    "mockjs": "^1.1.0",
    "postcss": "^8.5.3",
    "postcss-mobile-forever": "^4.4.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.8.2",
    "unocss": "^66.0.0",
    "vite": "^6.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock-dev-server": "^1.8.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^3.0.7"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no-install commitlint --edit $1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
